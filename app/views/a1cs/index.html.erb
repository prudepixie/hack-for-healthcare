<p id="notice"><%= notice %></p>

<h1>Listing A1cs</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @a1cs.each do |a1c| %>
      <tr>
        <td><%= a1c.value %></td>
        <td><%= link_to 'Show', a1c %></td>
        <td><%= link_to 'Edit', edit_a1c_path(a1c) %></td>
        <td><%= link_to 'Destroy', a1c, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New A1c', new_a1c_path %>
